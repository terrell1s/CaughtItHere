        <br />
        <input type="submit" value="Search">
    </form>
    <script>
        function checkAll(eventInfo)
        {
            var option = document.getElementById("selectFish");
            for (var i = 0; i < option.length; i++) {
                option[i].checkAll = event.srcElement.checked;

            }
        }
    </script>
</div>


















